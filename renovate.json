{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "node": {
    "enabled": true
  },
  "nvm": {
    "enabled": true
  },
  "js": {
    "enabled": true,
    "ignoreDeps": []
  },
  "autodiscover": true,
  "autodiscoverFilter": "carynligon/renovate-test",
  "onboarding": false,
  "requireConfig": false,
  "groupName": "all dependencies",
  "separateMajorMinor": false,
  "groupSlug": "all",
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "all dependencies",
      "groupSlug": "all"
    }
  ]
}
